<%- contentFor('body') %>

  <h3 class='highlight bold-italic-text'>POKEMON PROPERTIES</h3>
  <table class='highlight bold-italic-text'>
    <thead>
      <tr class="table-rows">
        <th>NAME</th>
        <th>ID</th>
        <th>TRAINER NAME</th>
        <th>TRAINER ID</th>
        <th> COMBAT POWER</th>
        <th> IN GYM</th>
      </tr>
    </thead>
    <tbody>
      <tr class="table-rows">
        <td><%= pokemonProperties.name %></td>
        <td><%= pokemonProperties.id %></td>
        <td><%= trainer.name %></td>
        <td><%= pokemonProperties.trainer_id %></td>
        <td><%= pokemonProperties.cp %></td>
        <td><%= pokemonProperties.in_gym %></td>
      </tr>
    </tbody>
  </table>
